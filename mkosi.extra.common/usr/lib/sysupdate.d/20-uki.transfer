# SPDX-License-Identifier: LGPL-2.1-or-later

[Transfer]
ProtectVersion=%A
Verify=no

[Source]
Type=url-file
Path=https://files.snowlinux.org/
MatchPattern=%M_@v_%a.efi

[Target]
Type=regular-file
Path=/EFI/Linux
PathRelativeTo=boot
MatchPattern=%M_@v_%a+@l-@d.efi \
             %M_@v_%a+@l.efi \
             %M_@v_%a.efi \
             %M_@v+@l-@d.efi \
             %M_@v+@l.efi \
             %M_@v.efi
Mode=0600
TriesLeft=3
TriesDone=0
InstancesMax=2
