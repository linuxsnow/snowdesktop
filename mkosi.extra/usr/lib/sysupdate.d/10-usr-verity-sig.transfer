[Transfer]
ProtectVersion=%A
Verify=networkd

[Source]
Type=url-file
Path=https://files.snowlinux.org/
MatchPattern=%M_@v_%a.usr-%a-verity-sig.@u.raw

[Target]
Type=partition
Path=auto
MatchPattern=%M_@v_verity_sig
MatchPartitionType=usr-verity-sig
PartitionFlags=0
ReadOnly=1
