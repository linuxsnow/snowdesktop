[Transfer]
ProtectVersion=%A
Verify=no

[Source]
Type=url-file
Path=https://files.snowlinux.org/
MatchPattern=%M_@v_%a.usr-%a-verity.@u.raw

[Target]
Type=partition
Path=auto
MatchPattern=%M_@v_verity
MatchPartitionType=usr-verity
PartitionFlags=0
ReadOnly=1
