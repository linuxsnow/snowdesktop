[Transfer]
ProtectVersion=%A

[Source]
Type=regular-file
Path=/
PathRelativeTo=explicit
MatchPattern=%M_@v_%a.usr-%a-verity-sig.@u.raw

[Target]
Type=partition
Path=auto
MatchPattern=%M_@v_verity_sig
MatchPartitionType=usr-verity-sig
PartitionFlags=0
ReadOnly=1
